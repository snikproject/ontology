he:StrategischeSystementwicklung
    he:page "127"^^xsd:positiveInteger ;
    meta:chapter he:SPLAN ;
    meta:consolidated false ;
    meta:functionComponent he:AnalyseUndEvaluationEntwicklungsrueckstau, he:AnalyseUndEvaluationVonProjektideen, he:Bestandsmanagement, he:ControllingderSystemabwicklung, he:Systemanalyse, he:Systementwurf, he:Systemimplementierung, he:VergebenvonPrioritaetenfuerdieProjektrealisierung ;
    meta:subTopClass meta:EntityType, meta:Function ;
