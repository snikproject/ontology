ob:BusinessProcess
    ob:page "156"^^xsd:positiveInteger ;
    meta:chapter ob:Chapter8-3 ;
    meta:entityTypeComponent ob:Activity, ob:ControlStructure, ob:Event, ob:InformationObject, ob:ProcessRole, ob:Tool ;
