    meta:entityTypeComponent bb:DescriptionOfTheCurrentHISState, bb:DescriptionOfThePlannedHISState, bb:DirectiveForHISConstruction, bb:MigrationPath, bb:StrategicHospitalGoal, bb:StrategicInformationManagementGoal ;
